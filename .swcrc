{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "baseUrl": "./",
    "parser": {
      "syntax": "typescript",
      "decorators": false
    },
    "target": "es2022",
    "keepClassNames": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "module": {
    "type": "es6",
    "strict": true,
    "noInterop": true,
    "resolveFully": true
  },
  "minify": false
}
